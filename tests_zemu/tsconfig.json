{
  "compilerOptions": {
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "declaration": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "outDir": "./dist",
    "lib": [
      "es2020"
    ],
    "target": "es2020",
  },
  "exclude": [
    "node_modules",
    "./dist/**"
  ]
}
